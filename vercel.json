{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://www.alanboulesteix.com https://nft-preview-card-component-git-main-boulesteixs-projects.vercel.app"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://www.alanboulesteix.com https://nft-preview-card-component-git-main-boulesteixs-projects.vercel.app"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "no-referrer"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(self), microphone=()"
        }
      ]
    }
  ]
}